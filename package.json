{
  "name": "grapheditor",
  "description": "An awesome requirejs app",
  "version": "0.0.0",
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "production": "./node_modules/.bin/webpack --config webpack.config.prod.js",
    "development": "./node_modules/.bin/webpack --config webpack.config.js --watch",
    "browser-sync": "./node_modules/.bin/browser-sync start --serveStatic '.' --port 5000 --files 'dist' ",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": {
    "name": "Filip Jelic",
    "email": "jelic.filip@gmail.com",
    "url": "http://www.filipjelic.com"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://mit-license.org"
    }
  ],
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.18.8",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "jest": "^20.0.1",
    "rimraf": "^2.6.1",
    "webpack": "^2.5.1"
  },
  "dependencies": {
  },
  "jest": {
    "verbose": true,
    "modulePaths": [
      "<rootDir>/app"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "bower_components"
    ]
  }
}
